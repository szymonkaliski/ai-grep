{
  "name": "ai-grep",
  "version": "1.1.0",
  "description": "AI-powered fuzzy grep using Claude",
  "type": "module",
  "bin": {
    "aig": "./dist/bin/aig.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run typecheck && npm run build",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ai",
    "search",
    "grep",
    "claude",
    "fuzzy"
  ],
  "author": "Szymon Kaliski <hi@szymonkaliski.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szymonkaliski/ai-grep.git"
  },
  "bugs": {
    "url": "https://github.com/szymonkaliski/ai-grep/issues"
  },
  "homepage": "https://github.com/szymonkaliski/ai-grep#readme",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "prettier": "^3.4.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
